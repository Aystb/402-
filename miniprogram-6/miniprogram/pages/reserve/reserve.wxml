<!--预约界面pages/reserve/reserve.wxml-->
<view class="top">预约信息</view>
<view class="box">
<view class="contain">
<view ></view>
  <view>日期</view>
  <view>开始时间</view>
  <view>结束时间</view>
  <view>选择事项</view>
  <view>使用详细说明</view>
</view>
<view class="contain" id="contain">
  <input type="text" class="input" bindinput="reserveData" />
  <input type="text" class="input" bindinput="starttime"  />
  <input type="text" class="input" bindinput="endtime" />
  <!-- 这里是对活动进行选择，有例会和活动和其他 -->
  <select name="" id="">
  <option value="">例会</option>
  <option value="">活动</option>
  <option value="">其他</option>
  </select>
  <input type="text" class="input" bindinput="text" />


</view>
</view>
<button type="primary" bindtap="submit">提交</button>